.table-bordered {
  
  border-style: solid;
  border-radius: 5px;
  border-collapse: collapse;
}

.table-bordered th,
.table-bordered td {
  min-width: 80px;
  padding: 10px;
  line-height: 16px;
  text-align: center;
  vertical-align: top;
  border: 1px solid black;
}


